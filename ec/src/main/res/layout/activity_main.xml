<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.ec.MainActivity">


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Http"
        android:id="@+id/button"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true" />

    <Button
        android:text="new api"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button"
        android:layout_alignParentLeft="true"
        android:id="@+id/btn_newapi"
        android:layout_alignParentRight="true" />

    <Button
        android:text="greendao"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btn_newapi"
        android:layout_alignParentLeft="true"
        android:id="@+id/btn_greendao"
        android:layout_alignParentRight="true" />

    <Button
        android:text="update_app"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btn_greendao"
        android:layout_alignParentLeft="true"
        android:id="@+id/btn_update_app"
        android:layout_alignParentRight="true" />

    <Button
        android:text="Zxing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btn_update_app"
        android:layout_alignParentLeft="true"
        android:id="@+id/btn_scan"
        android:layout_alignParentRight="true" />


</RelativeLayout>
